<?xml version='1.0'?>
<launch>

  <!-- Configuration of Jackal which you would like to simulate.
       See jackal_description for details. -->
  <arg name="config" default="front_laser" />

  <!-- Load Jackal's description, controllers, and teleop nodes. -->
  <include file="$(find example_robot_description)/launch/description.launch">
    <arg name="config" value="$(arg config)" />
  </include>

  <include file="$(find example_control)/launch/control.launch" />

  <!-- This line will spawn oyur robot in the correct starting location -->
  <!-- Replace "example_robot" by the name of your robot -->
  <include file="$(find virtual_maize_field)/launch/robot_spawner.launch">
    <arg name="robot_name" value="example_robot"/>
  </include>

</launch>
